  },
  "host": "localhost:3000",
  "basePath": "/api",
  "swagger": "2.0",
  "paths": {
    "attack/Fire": {
      "post": {
        "description": "Attacks to the planet with given weapon.",
        "produces": [
          "application/json"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "params",
            "schema": {
              "type": "object",
              "required": [
                "weaponName",
                "planetName"
              ],
              "properties": {
                "weaponName": {
                  "type": "string",
                  "default": "Death Star"
                },
                "planetName": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Example attack result"
          },
          "422": {
            "description": "Missing parameters"
          }
        }
      }
    },
    "/planet/Defend": {
      "post": {
        "description": "Attacks to the planet with given weapon.",
        "produces": [
          "application/json"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "params",
            "schema": {
              "type": "object",
              "required": [
                "weaponName",
                "planetName"
              ],
              "properties": {
                "weaponName": {
                  "type": "string",
                  "default": "Death Star"
                },
                "planetName": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Example attack result"
          },
          "422": {
            "description": "Missing parameters"
          }
        }
      }
    }
  },
